{
    "functions": {
        "api/generate.js": {
            "maxDuration": 30
        },
        "api/names.js": {
            "maxDuration": 30
        }
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        },
        {
            "source": "/api/names",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=300, stale-while-revalidate=600"
                }
            ]
        }
    ]
}